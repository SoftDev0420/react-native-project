{
  "compilerOptions": {
    "target": "ESNEXT",
    "moduleResolution": "Node",
    "module": "esnext",
    "sourceMap": true,
    "noUnusedLocals": false,
    "rootDir": "prisma/src",
    "outDir": "prisma/dist"
  },    
  "include": [
    "prisma/src/**/*.ts",
    "prisma/src/**/*.js",
    "prisma/src/*.js",
    ],
    "exclude": [
        "node_modules",
        "**/node_modules",
        "**/*.spec.ts"
    ]
}
